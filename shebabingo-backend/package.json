{
  "name": "shebabingo-backend",
  "version": "1.0.0",
  "description": "Backend server for ShebaBingo multiplayer game with Telegram integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"No tests yet\" && exit 0",
    "backup": "pg_dump $DATABASE_URL > backups/shebabingo_$(date +%Y%m%d_%H%M%S).sql",
    "restore": "psql $DATABASE_URL < backups/latest.sql",
    "test-db": "node database/test-connection.js",
    "load-test": "node database/load-test.js",
    "init-db": "psql -U postgres -d shebabingo -f database/schema.sql"
  },
  "keywords": [
    "bingo",
    "multiplayer",
    "telegram",
    "game",
    "real-time"
  ],
  "author": "Your Name",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.67.0",
    "pg": "^8.16.3",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  },
  
  "engines": {
    "node": ">=16.0.0"
  }
}
